{
  "$schema": "https://schema.tauri.app/config/2",
  "build": {
    "beforeDevCommand": "yarn vite --clearScreen=false",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "yarn exec vite build",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Stump",
  "mainBinaryName": "Stump",
  "version": "0.0.0",
  "identifier": "com.oromei.stump",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "height": 700,
        "resizable": true,
        "title": "Stump",
        "width": 1200,
        "decorations": true,
        "transparent": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}