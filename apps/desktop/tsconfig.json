{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": [
      "vite/client"
    ],
    "outDir": "../../.moon/cache/types/apps/desktop",
    "paths": {
      "@stump/client": [
        "../../packages/client/src/index.ts"
      ],
      "@stump/client/*": [
        "../../packages/client/src/*"
      ],
      "@stump/interface": [
        "../../packages/interface/src/index.ts"
      ],
      "@stump/interface/*": [
        "../../packages/interface/src/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "../../packages/client"
    },
    {
      "path": "../../packages/interface"
    }
  ]
}
