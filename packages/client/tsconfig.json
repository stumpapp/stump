{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"jsx": "preserve",
		"skipLibCheck": true,
		"outDir": "../../.moon/cache/types/packages/client",
		"paths": {
			"@stump/api": [
				"../api/src/index.ts"
			],
			"@stump/api/*": [
				"../api/src/*"
			],
			"@stump/types": [
				"../types/index.ts"
			],
			"@stump/types/*": [
				"../types/*"
			]
		}
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	],
	"references": [
		{
			"path": "../api"
		},
		{
			"path": "../types"
		}
	]
}
