{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": [
      "vite/client",
      "node"
    ],
    "outDir": "../../.moon/cache/types/packages/interface",
    "skipLibCheck": true,
    "paths": {
      "@stump/client": [
        "../client/src/index.ts"
      ],
      "@stump/client/*": [
        "../client/src/*"
      ]
    },
    "resolveJsonModule": true
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "../client"
    }
  ]
}
